{
  "name": "react_redux_template",
  "version": "0.0.0",
  "description": "Sample project for React & Redux",
  "main": "src/index.js",
  "scripts": {
    "build": "NODE_PATH=. webpack",
    "build:production": "NODE_PATH=. webpack --config webpack.production.config.js -p",
    "lint": "eslint --rulesdir ./eslint_rules --ext .js --ext .jsx ./",
    "lint:fix": "eslint --rulesdir eslint_rules --fix --ext .js --ext .jsx ./",
    "serve": "NODE_PATH=. NODE_ENV=development webpack-dev-server --host 0.0.0.0 --public lvh.me:8080",
    "test": "npm run test:file -- './**/*_specs.@(js|jsx)'",
    "test:file": "NODE_PATH=. NODE_ENV=test mocha --compilers js:babel-core/register,js:babel-polyfill,css:src/utils/css_test_compiler --require ./src/utils/test_helper.js",
    "test:coverage": "NODE_PATH=. NODE_ENV=test node -r babel-core/register node_modules/.bin/isparta cover --include '**/*.js' --include '**/*.jsx' --report text --report lcov -x '*_specs.@(js|jsx)' _mocha -- --compilers js:babel-core/register,js:babel-polyfill,css:src/utils/css_test_compiler --recursive --require './src/utils/test_helper.js' 'src/**/*_specs.*'",
    "posttest:coverage": "open .coverage/lcov-report/index.html",
    "test:watch": "npm run test -- --watch --watch-extensions jsx"
  },
  "repository": {
    "url": "git+ssh://git@bitbucket.org:signalnoise/react_redux_project_template.git",
    "type": "git"
  },
  "keywords": [
    "react redux project template"
  ],
  "author": "Zdenek Hynek <zdenek@signal-noise.co.uk>",
  "license": "UNLICENSED",
  "homepage": "https://bitbucket.org/signalnoise/react_redux_project_template/#readme",
  "devDependencies": {
    "autoprefixer": "^6.5.1",
    "babel-core": "^6.18.0",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^6.2.7",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2016": "^6.16.0",
    "babel-preset-es2017": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "chai-enzyme": "^0.5.2",
    "chai-immutable": "^1.6.0",
    "chance": "^1.0.4",
    "css-loader": "^0.25.0",
    "css-modules-require-hook": "^4.0.5",
    "enzyme": "^2.5.1",
    "eslint": "^3.9.1",
    "eslint-plugin-react": "^6.4.1",
    "file-loader": "^0.9.0",
    "handlebars-loader": "^1.4.0",
    "html-webpack-plugin": "^2.24.1",
    "immutable-devtools": "0.0.7",
    "isparta": "^4.0.0",
    "istanbul": "0.4.4",
    "jsdom": "^9.8.3",
    "jsx-chai": "^4.0.0",
    "mocha": "^3.1.2",
    "nock": "^9.0.2",
    "postcss-import": "7.1.3",
    "postcss-loader": "^1.1.0",
    "postcss-simple-vars": "^3.0.0",
    "precss": "^1.4.0",
    "react-addons-test-utils": "^15.3.2",
    "react-hot-loader": "^1.3.0",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.1.1",
    "rosie": "^1.6.0",
    "sinon": "^1.17.6",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.4"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "eslint-plugin-signalnoise": "file:./eslint_rules",
    "immutable": "^3.8.1",
    "isomorphic-fetch": "^2.2.1",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-immutable-proptypes": "^2.1.0",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.2",
    "redux": "^3.6.0",
    "redux-immutable": "^4.0.0",
    "redux-thunk": "^2.1.0"
  },
  "engines": {
    "node": ">=6.7.0",
    "npm": "3.10.7"
  },
  "private": true
}
